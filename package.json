{
  "name": "naom-shopify",
  "version": "1.0.0",
  "description": "NAOM shopify theme",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "run-p tw-watch theme-dev",
    "theme-dev": "shopify theme dev",
    "theme-check": "shopify theme check",
    "theme-push": "run-s tw && shopify theme push",
    "theme-pull": "run-s shopify theme pull && tw",
    "tw": "npx tailwindcss -i ./tailwind-input.css -o ./assets/tailwind-output.css",
    "tw-watch": "npx tailwindcss -i ./tailwind-input.css -o ./assets/tailwind-output.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@shopify/cli": "^3.57.0",
    "@shopify/theme": "^3.57.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.1"
  }
}
